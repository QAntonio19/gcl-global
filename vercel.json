{
  "version": 2,
  "builds": [
    {
      "src": "**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/contact-us",
      "dest": "/contact-us/index.html"
    },
    {
      "src": "/contact-us/",
      "dest": "/contact-us/index.html"
    },
    {
      "src": "/growth-driven-design",
      "dest": "/growth-driven-design/index.html"
    },
    {
      "src": "/growth-driven-design/",
      "dest": "/growth-driven-design/index.html"
    },
    {
      "src": "/inbound-marketing",
      "dest": "/inbound-marketing/index.html"
    },
    {
      "src": "/inbound-marketing/",
      "dest": "/inbound-marketing/index.html"
    },
    {
      "src": "/wave-of-social-media-overwhelms-digital-marketing-in-2018",
      "dest": "/wave-of-social-media-overwhelms-digital-marketing-in-2018/index.html"
    },
    {
      "src": "/wave-of-social-media-overwhelms-digital-marketing-in-2018/",
      "dest": "/wave-of-social-media-overwhelms-digital-marketing-in-2018/index.html"
    },
    {
      "src": "/how-can-a-growth-driven-design-optimize-your-website",
      "dest": "/how-can-a-growth-driven-design-optimize-your-website/index.html"
    },
    {
      "src": "/how-can-a-growth-driven-design-optimize-your-website/",
      "dest": "/how-can-a-growth-driven-design-optimize-your-website/index.html"
    },
    {
      "src": "/inbound-marketing-a-coming-of-age-technique-loved-by-all",
      "dest": "/inbound-marketing-a-coming-of-age-technique-loved-by-all/index.html"
    },
    {
      "src": "/inbound-marketing-a-coming-of-age-technique-loved-by-all/",
      "dest": "/inbound-marketing-a-coming-of-age-technique-loved-by-all/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/css/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/js/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/fonts/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}

